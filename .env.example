DISCORD_BOT_TOKEN=
DISCORD_CLIENT_ID=
DISCORD_GUILD_ID=
OWNER_IDS=
DATABASE_URL="mysql://user:password@localhost:3306/discord_bot"
DEFAULT_TIMEZONE=Asia/Tokyo
LOG_CHANNEL_ID=

# Metrics / Influx settings
# Set to 'true' to collect memory/uptime fields
METRICS_DETAILED=true
# How often to create/send metrics (ms)
METRICS_SEND_INTERVAL_MS=10000
# How often to flush batched writes to Influx (ms)
INFLUX_FLUSH_INTERVAL_MS=10000
# Number of flush retries before giving up
INFLUX_FLUSH_RETRY_COUNT=3
# Backoff base ms for flush retries
INFLUX_FLUSH_RETRY_BASE_MS=500

# Pod metadata (set via Downward API in k8s)
POD_NAME=
POD_NAMESPACE=

INFLUX_URL=https://influxdb.skyia.jp
INFLUX_TOKEN=
INFLUX_ORG=
INFLUX_BUCKET=
